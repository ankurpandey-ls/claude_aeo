# AEO Analytics Environment Variables
# Copy this file to .env and fill in your actual API keys

# API Keys
EXA_API_KEY=your_exa_api_key_here
OPENAI_API_KEY=your_openai_api_key_here
OPENROUTER_API_KEY=your_openrouter_api_key_here
PERPLEXITY_API_KEY=your_perplexity_api_key_here

# Configuration
CONCURRENT_PROCESSING=true  # Set to false for sequential processing
MAX_RETRIES=3  # Number of retries for failed API calls

# Optional: Database configuration (if not using Replit DB)
# DATABASE_URL=sqlite:///./aeo_analytics.db
# DATABASE_URL=postgresql://user:password@localhost/aeo_analytics
